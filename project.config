[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Mandar Bapaye (mandarbapaye)
[access "refs/for/refs/*"]
	read = group user/Mandar Bapaye (mandarbapaye)
	push = group user/Mandar Bapaye (mandarbapaye)
	label-Code-Review = -2..+2 group user/Mandar Bapaye (mandarbapaye)
	label-Verified = -1..+1 group user/Mandar Bapaye (mandarbapaye)
	rebase = group user/Mandar Bapaye (mandarbapaye)
	removeReviewer = group user/Mandar Bapaye (mandarbapaye)
	submit = group user/Mandar Bapaye (mandarbapaye)
[access "refs/heads/*"]
	read = group user/Mandar Bapaye (mandarbapaye)
	create = group user/Mandar Bapaye (mandarbapaye)
	push = +force group user/Mandar Bapaye (mandarbapaye)
	pushMerge = group user/Mandar Bapaye (mandarbapaye)
[access "refs/tags/*"]
	push = +force group user/Mandar Bapaye (mandarbapaye)
	pushTag = group user/Mandar Bapaye (mandarbapaye)
	pushSignedTag = group user/Mandar Bapaye (mandarbapaye)
